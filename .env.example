# Application Configuration
VITE_APP_URL=http://localhost:5173
NODE_ENV=development

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_PROJECT_ID=your_project_id
VITE_SUPABASE_PUBLISHABLE_KEY=your_anon_key

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/auth/callback

# API Keys and Services
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_key
VITE_SLACK_CLIENT_ID=your_slack_client_id
VITE_WHATSAPP_BUSINESS_ID=your_whatsapp_id

# Calendar Integration
VITE_CALENDAR_API_KEY=your_calendar_api_key
VITE_CALENDAR_INTEGRATION_TYPE=google
VITE_CALENDAR_SYNC_INTERVAL=300

# Email Service Configuration
VITE_EMAIL_SERVICE=supabase_edge
VITE_EMAIL_FROM=noreply@cortexcloud.online
VITE_EMAIL_REPLY_TO=support@cortexcloud.online

# Feature Flags
VITE_ENABLE_WHATSAPP=false
VITE_ENABLE_SLACK=false
VITE_ENABLE_VIDEO_CALLS=true

# Performance and Optimization
VITE_ENABLE_PWA=true
VITE_CACHE_TTL=3600
VITE_MAX_UPLOAD_SIZE=10485760

# Deployment and Infrastructure
VITE_DEPLOYMENT_REGION=us-east-1
VITE_CDN_URL=https://cdn.cortexcloud.online
VITE_API_BASE_URL=https://api.cortexcloud.online